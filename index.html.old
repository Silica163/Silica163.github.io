<!doctype html>
<html>
  <head>
    <title>Silica163.github.io</title>
    <mata charset="utf-8"></meta>
    <style>
        :root{
          --bg:#232323;
          --fg:#eee;
        }
        ::selection{
          background-color: var(--fg);
          color: var(--bg);
        }
        html{
          background-color:var(--bg);
          color:var(--fg);
          margin:auto;
          font-family: consolas;
        }
        .center{
          position:absolute;
          top:50%;
          left:50%;
          transform:translate(-50%,-50%);
          min-width:500px;
          max-width:1000px;
        }
        input.inp{
          border: 2px solid #888;
          border-radius: 8px;
          margin:auto;
          background-color:var(--bg);
          color:var(--fg);
          height:30px;
          font-size: 20px;
        }
        input.num{
          width:100px;
          text-align: right;
        }
    </style>
  </head>
  <body>
    <h1 class=center>Silica163.github.io</h1>
  </body>
</html>
